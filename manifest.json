{
   "name": "Save image as Type",
   "short_name": "SaveImageAs",
   "description": "Save image as PNG or JPG by context menu on image.",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "manifest_version": 3,
   "minimum_chrome_version": "129.0.6668.70",
   "permissions": [
      "contextMenus", "scripting"
   ],
   "optional_permissions": [],
   "host_permissions": [
      "http://*/*", 
      "https://*/*" 
   ],
   "version": "1.2.4"
}
